<?php

namespace appNamespace\Http\Resources;

use Illuminate\Http\Resources\Json\JsonResource;

/** @property-read \modelClassFull $resource */
class modelClassResource extends JsonResource
{
    /**
     * Transform the resource into an array.
     *
     * @param  \Illuminate\Http\Request $request
     * @return array
     */
    public function toArray($request)
    {
        return [
            'id' => $this->resource->id,
        ];
    }
}
